/*收到简历页，面试状态列：start*/

.receive-listmain {
    margin: 0 0 1.75rem 0;
    font-size: .85rem;
}

.receive-listmain ul {
    background: #fff;
    list-style: none;
    padding: 0;
    margin: 0;
    position: relative;
}

.receive-listmain .receive-list {}

.receive-listmain .receive-item {
    padding-left: 0;
    box-sizing: border-box;
    min-height: 2.2rem;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    position: relative;
}

.receive-listmain .receive-item:after {
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    right: auto;
    top: auto;
    height: 1px;
    width: 100%;
    background-color: #e7e7e7;
    display: block;
    z-index: 1;
    -webkit-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
}

.receive-listmain .receive-main {
    position: relative;
    width: 100%;
    height: 3rem;
    margin: .5rem .5rem;
}

.receive-listmain .receive-headimg {
    width: 3rem;
    height: 3rem;
    border-radius: 50%;
    text-align: center;
    position: absolute;
    overflow: hidden;
    border: 2px solid #ddd;
}

.receive-listmain .receive-headimg img {
    vertical-align: middle;
    width: 100%;
    height: 100%;
}

.receive-listmain .receive-content {
    margin-left: 3.2rem;
    padding: .4rem 0;
}

.receive-listmain .receive-title {
    font-size: .8rem;
    color: #333;
}

.receive-listmain .receive-title .time {
    float: right;
    font-size: .7rem;
    color: #666;
    margin-top: .2rem;
}

.receive-listmain .receive-p {
    font-size: .7rem;
    position: relative;
    padding-right: 3rem;
}

.receive-listmain .receive-p .status {
    position: absolute;
    right: 0;
    top: 0;
}

.receive-listmain .receive-p .status::before {
    content: "[";
}

.receive-listmain .receive-p .status::after {
    content: "]";
}


