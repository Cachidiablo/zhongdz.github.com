/*覆盖sui 样式*/

.label-switch input[type=checkbox]:checked+.checkbox {
    background: #0088ce;
}

label.label-checkbox input[type="checkbox"]:checked ~ .item-inner,
label.label-checkbox input[type="radio"]:checked ~ .item-inner {
    color: #0088ce;
}

label.label-checkbox input[type="checkbox"]:checked ~ .item-inner .item-hidden-element,
label.label-checkbox input[type="radio"]:checked ~ .item-inner .item-hidden-element {
    display: block;
}

.item-hidden-element {
