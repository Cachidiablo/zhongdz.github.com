/*上传图片样式：start*/

.wl-uploader-primary {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -ms-flex: 1;
    flex: 1;
}

.wl-uploader-hd {
    padding-top: 0;
    padding-right: 0;
    padding-left: 0;
}

.wl-uploader-bd ul.wl-uploader-files {
    padding: 0;
}

.wl-cell {
    padding: 10px 0px;
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;
}

.wl-cell {
    position: relative;
}

.wl-uploader-hd .wl-cell-ft {
    font-size: 1em;
}

.wl-cells-form .wl-cell-ft {
    font-size: 0;
}

.wl-cell-ft {
    text-align: right;
    color: #888;
}

.wl-uploader-bd {
    margin-bottom: -4px;
    margin-right: -9px;
    overflow: hidden;
}

.wl-uploader-files {
    list-style: none;
}

.wl-uploader-file {
    float: left;
    margin-right: 9px;
    margin-bottom: 9px;
    width: 79px;
    height: 79px;
    background: no-repeat center center;
    background-size: cover;
}

.wl-uploader-status {
    position: relative;
}

.wl-uploader-status:before {
    content: " ";
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.5);
}

.wl-uploader-input-wrp {
    float: left;
    position: relative;
    margin-right: 9px;
    margin-bottom: 9px;
    width: 77px;
    height: 77px;
    border: 1px solid #D9D9D9;
}

.wl-uploader-input-wrp:before {
    width: 2px;
    height: 39.5px;
}

.wl-uploader-input-wrp:before,
.wl-uploader-input-wrp:after {
    content: " ";
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: #D9D9D9;
}

.wl-cells-form input,
.wl-cells-form textarea,
.wl-cells-form label[for] {
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

.wl-uploader-input {
    position: absolute;
    z-index: 1;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

.wl-uploader-input-wrp:after {
    width: 39.5px;
    height: 2px;
}


