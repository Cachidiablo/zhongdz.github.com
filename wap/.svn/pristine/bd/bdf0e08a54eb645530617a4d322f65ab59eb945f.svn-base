.custom_panel_top{
    margin-top: 40px;
}
.custom_panel_top a{
    margin-left: 10px;
}
.custom_panel_top span{
    margin-top: 5px;
    margin-left: 10px;
}
.card_list_parent{
    top: 4.5rem !important;
}
.left0{
    margin-left: 0rem !important;
}
.list-container{
    background-color: #f2f2f2 !important;
}
.list-container li{
    margin-top: 20px;
}
.card_bottom_parent a:nth-child(1){
    color: red;
    font-size: 0.8rem;
}
.card_bottom_parent span{
    font-size: 24px;
}
.card_bottom_parent a:nth-child(2){
    /*margin-left: -11rem;*/
}
.card_bottom_parent a:nth-child(3){
    width: 4.5rem;
    height: 2rem;
    border-radius: 0.8rem;
    overflow: hidden;
    text-align: center;
    line-height: 2rem;
    border-color: red;
    color: red;
    padding-left: 1.1rem;
}
.border{
    border: 1px solid red;
}
/* deal */
.barStyle{
    height: 2.2rem;
}
.barStyle a{
    line-height: 1.2rem !important;
    background-color: rgba(0, 0, 0, 0.3);
    border-radius: 100%;
    padding: 0.2rem 0.4rem !important;
    margin-top: 0.3rem;
}
.barStyle a:before{
    margin-left: -0.2rem;
    color: white;
}
.barStyle .title{
    line-height: 2.2rem !important;
    font-size: 1.2rem;
    font-family: "微软雅黑";
}
.whiteColor{
    background-color: white;
}
.bottomNavColor{
    background-color: rgb(123, 123, 123);
}
.bottomNavColor span{
    color: white;
}
.mash{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0px;
    background-color: rgba(123, 123, 123, 0.6);
}
/* deal */